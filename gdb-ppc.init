define print-pde 
	set $width = 0
	set $p = $arg0
	while ($p != 0)
		set $len = $p->length
		set $i = 0
		while ($i < $len)
			printf "%c", $p->name[$i]
			set $i = $i + 1
		end
		set $width = $width + $len + 3
		if ($width > 72)
			printf "\n"
			set $width = 0
		else
			printf "   "
		end
		set $p = $p->link
	end
end
